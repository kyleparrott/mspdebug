<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="https://raw.githubusercontent.com/kyleparrott/mspdebug/master/mspdebug.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>mspdebug</name>
  <summary>Debugging tool for MSP430's</summary>

  <!-- Optionally, uncomment this to specify the program's homepage: -->
  <homepage>http://dlbeer.co.nz/mspdebug/</homepage>

    <requires interface="https://raw.githubusercontent.com/kyleparrott/libmsp430/libmsp430.xml">
        <environment insert="lib" mode="prepend" name="LD_LIBRARY_PATH"/>
    </requires>

    <group arch="MacOSX-*" main="mspdebug">
        <implementation id="sha1=57d7dfc6a2a17b432476dbf24986b04f2f9c50ab" version="0.0">
            <archive href="https://raw.githubusercontent.com/kyleparrott/mspdebug/master/macos/mspdebug.zip" size="177567"/>
        </implementation>
    </group>
    <group arch="Linux-*" main="mspdebug">
         <implementation id="sha1=12b661b93801580836997b2488e4bcaed7968f6e" version="0.0">
             <archive href="https://raw.githubusercontent.com/kyleparrott/mspdebug/master/linux/mspdebug.zip" size="107451"/>
        </implementation>
    </group>
</interface>
